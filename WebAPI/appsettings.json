{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "AccessTokenExpiryMinutes": 15,
    "RefreshTokenExpiryMinutes": 60,
    "PersistentRefreshTokenExpiryDays": 30,
    "Issuer": "WebAPI.Dev",
    "Audience": "WebAPI.Client"
  }
}
